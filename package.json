{
  "name": "react-babel-webpack-boilerplate",
  "version": "1.4.1",
  "description": "A boilerplate for React-Babel-Webpack project",
  "main": "app/main.jsx",
  "scripts": {
    "lint": "eslint --cache --ignore-path .gitignore --ext .jsx,.js --format=node_modules/eslint-formatter-pretty .",
    "lint:msg": "validate-commit-msg",
    "test": "npm run lint",
    "build": "webpack",
    "start": "webpack-dev-server --devtool eval --progress --hot --content-base app",
    "deploy": "NODE_ENV=production webpack --config webpack.production.config.js",
    "deploy-windows": "SET NODE_ENV=production & webpack --config webpack.production.config.js",
    "validate": "npm ls",
    "commit": "git cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "cloudfront:deploy": "npm run build:cordova:prod && s3cmd sync --no-mime-magic --guess-mime-type --delete-removed cordova/www/* s3://orloff.phorque.it",
    "build:cordova": "rimraf cordova/www && webpack -d --progress --env.BUILD_TARGET=cordova",
    "build:cordova:prod": "rimraf cordova/www && NODE_ENV=production webpack --config webpack.production.config.js --progress --env.BUILD_MODE=prod --env.BUILD_TARGET=cordova && cd cordova && cordova build",
    "build:cordova:release": "rimraf cordova/www && NODE_ENV=production webpack --config webpack.production.config.js --progress --env.BUILD_MODE=prod --env.BUILD_TARGET=cordova && cd cordova && cordova build --release && jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore ~/Documents/my-release-key.keystore platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk alias_name && rm platforms/android/app/build/outputs/apk/release/app-release.apk && zipalign -v 4 platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk platforms/android/app/build/outputs/apk/release/app-release.apk && cp platforms/android/app/build/outputs/apk/release/app-release.apk ~/Documents/."
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "6.x",
    "currency-symbol-map": "^4.0.3",
    "es6-promise": "^4.2.4",
    "exports-loader": "^0.6.4",
    "fetch-jsonp": "^1.1.3",
    "imports-loader": "^0.7.1",
    "material-ui": "^0.20.0",
    "material-ui-fullscreen-dialog": "^0.7.2",
    "moment": "^2.20.1",
    "object-from-entries": "^1.0.0",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "react": "^16.0.0",
    "react-dom": "^16.2.1",
    "react-redux": "^5.0.6",
    "react-responsive": "^4.0.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-page-transition": "^3.0.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "recharts": "^1.0.0-beta.9",
    "redux": "^3.7.2",
    "redux-form": "^7.1.1",
    "redux-localstorage": "^0.4.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "whatwg-fetch": "^2.0.3",
    "wtf_wikipedia": "^2.5.0"
  },
  "devDependencies": {
    "babel-core": "6.x",
    "babel-eslint": "7.x",
    "babel-loader": "6.x",
    "babel-plugin-transform-runtime": "6.x",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.x",
    "babel-preset-stage-0": "^6.24.1",
    "commitizen": "^2.9.3",
    "conventional-changelog-cli": "1.x",
    "copy-webpack-plugin": "latest",
    "css-loader": "~0.26.1",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-compat": "^1.0.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "3.x",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "latest",
    "file-loader": "^1.1.6",
    "husky": "^0.12.0",
    "image-webpack-loader": "^3.4.2",
    "open-browser-webpack-plugin": "0.0.3",
    "style-loader": "~0.13.1",
    "svg-inline-loader": "^0.8.0",
    "url-loader": "^0.6.2",
    "validate-commit-msg": "2.x",
    "webpack": "latest",
    "webpack-dev-server": "latest"
  },
  "keywords": [
    "es6",
    "web app",
    "boilerplate"
  ],
  "author": "Ruan Yifeng",
  "license": "MIT",
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
